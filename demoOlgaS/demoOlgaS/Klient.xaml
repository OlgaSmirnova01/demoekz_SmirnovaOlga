<Window x:Class="demoOlgaS.Klient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demoOlgaS"
        mc:Ignorable="d"
        Title="Klient" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="23*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="Ivory">
            <TextBlock Text="Клиенты" Style="{StaticResource zag}"/>
        </StackPanel>
        <StackPanel Grid.Row="2" Background="Khaki"></StackPanel>
    </Grid>
</Window>
